  "authors": [
    {
      "name": "sylvxa",
      "contact": {
        "email": "me@sylvie.lol",
        "homepage": "https://sylvie.lol"
      }
    }
  ],
  "contact": {
    "repo": "https://github.com/sylvxa/sswaystones",
    "homepage": "https://modrinth.com/mod/sswaystones",
    "issues": "https://github.com/sylvxa/sswaystones/issues",
    "discord": "https://discord.gg/5qzrB98sBA"
  },
  "license": "MIT",
  "icon": "assets/sswaystones/icon.png",
